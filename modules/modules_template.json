[
    {
        "id": "okapi",
        "repo": "https://github.com/folio-org/okapi.git",
        "tag": "v6.1.0",
        "build": "mvn clean install -DskipTests",
        "rebuild": "false"
    },
    {
        "id": "mod-permissions",
        "tag": "v6.6.1",
        "enabled": "true"
    },
    {
        "id": "mod-inventory-storage",
        "repo": "https://github.com/folio-org/mod-inventory-storage.git",
        "tag": "v28.0.4",
        "step": "install",
        "build": "mvn clean install -DskipTests -Dcheckstyle.skip",
        "rebuild": "false",
        "env": [
            {
                "name": "DB_HOST",
                "value": "localhost"
            },
            {
                "name": "DB_PORT",
                "value": "5432"
            },
            {
                "name": "DB_USERNAME",
                "value": "folio_admin"
            },
            {
                "name": "DB_PASSWORD",
                "value": "folio_admin"
            },
            {
                "name": "DB_DATABASE",
                "value": "okapi_modules_staging"
            },
            {
                "name": "DB_QUERYTIMEOUT",
                "value": "60000"
            },
            {
                "name": "DB_MAXPOOLSIZE",
                "value": "5"
            }
        ],
        "enabled": "true"
    },
    {
        "id": "mod-users",
        "tag": "v19.4.4",
        "step": "install",
        "build": "mvn clean install -DskipTests -Dcheckstyle.skip",
        "rebuild": "false",
        "enabled": "true"
    },
    {
        "id": "mod-configuration",
        "tag": "v5.11.0",
        "step": "install",
        "build": "mvn clean install -DskipTests -Dcheckstyle.skip",
        "rebuild": "false",
        "enabled": "true"
    },
    {
        "id": "mod-login",
        "tag": "v7.12.1",
        "step": "install",
        "build": "mvn clean install -DskipTests -Dcheckstyle.skip",
        "rebuild": "false",
        "enabled": "true"
    },
    {
        "id": "mod-password-validator",
        "tag": "v3.3.0",
        "postman": {
            "file": "src/main/resources/swagger.api/password-validator.yaml",
            "api_key": "PMAK-xxxxxxxxxxxxxxxxxxxxxx-xxxxxxxxxxxxxxxxxxxxxxx",
            "enabled": "false"
        },
        "enabled": "true"
    },
    {
        "id": "mod-authtoken",
        "tag": "v2.16.1",
        "step": "install",
        "build": "mvn clean install -DskipTests -Dcheckstyle.skip",
        "rebuild": "false",
        "env": [
            {
                "name": "TOKEN_EXPIRATION_SECONDS",
                "value": "accessToken:600,refreshToken:604800"
            }
        ],
        "enabled": "true"
    },
    {
        "id": "mod-pubsub",
        "tag": "v2.15.3",
        "step": "install",
        "build": "mvn -DskipTests -Dcheckstyle.skip install",
        "rebuild": "false",
        "env": [
            {
                "name": "KAFKA_HOST",
                "value": "localhost"
            },
            {
                "name": "KAFKA_PORT",
                "value": "9092"
            },
            {
                "name": "NUMBER_OF_PARTITIONS",
                "value": "1"
            },
            {
                "name": "KAFKA_PRODUCER_TENANT_COLLECTION",
                "value": "ALL"
            },
            {
                "name": "ENV",
                "value": "folio"
            },
            {
                "name": "SYSTEM_USER_NAME",
                "value": "pub-sub"
            },
            {
                "name": "SYSTEM_USER_PASSWORD",
                "value": "pubsub"
            }
        ],
        "enabled": "true"
    },
    {
        "id": "mod-email",
        "tag": "v1.18.0",
        "enabled": "false"
    },
    {
        "id": "mod-users-bl",
        "tag": "v7.9.3",
        "step": "install",
        "build": "mvn -DskipTests -Dcheckstyle.skip install",
        "rebuild": "false",
        "enabled": "true"
    },
    {
        "id": "mod-settings",
        "step": "install",
        "tag": "v1.1.0",
        "enabled": "false"
    },
    {
        "id": "mod-inventory",
        "step": "install",
        "tag": "v21.0.5",
        "okapi": {
            "url": "https://folio-snapshot-okapi.dev.folio.org",
            "tenant": "diku",
            "credentials": {
                "username": "diku_admin",
                "password": "admin"
            },
            "enabled": "false"
        },
        "enabled": "true"
    },
    {
        "id": "mod-search",
        "step": "install",
        "tag": "v4.0.7",
        "env": [
            {
                "name": "SYSTEM_USER_ENABLED",
                "value": "true"
            },
            {
                "name": "SYSTEM_USER_NAME",
                "value": "mod-search"
            },
            {
                "name": "SYSTEM_USER_PASSWORD",
                "value": "Mod-search-1-0-0"
            }
        ],
        "enabled": "true"
    },
    {
        "id": "mod-reporting",
        "repo": "https://github.com/folio-org/mod-reporting.git",
        "step": "clone",
        "tag": "v1.2.1",
        "enabled": "true"
    }
]
